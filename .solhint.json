{
  "extends": "solhint:recommended",
  "rules": {
    "compiler-version": ["error", "^0.8.11"],
    "avoid-low-level-calls": "off",
    "avoid-throw": false,
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "func-visibility": ["off", { "ignoreConstructors": false }]
  },
  "plugins": ["prettier"]
}
